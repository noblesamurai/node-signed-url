    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "jsdoc-to-markdown": "^5.0.1",
    "mocha": "^6.2.1",
    "nyc": "^14.1.1",
    "semistandard": "*",
